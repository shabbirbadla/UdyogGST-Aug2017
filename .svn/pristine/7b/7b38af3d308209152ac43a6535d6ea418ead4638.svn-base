Procedure vou_common
Parameters tprogname,tbar,tpad	
	_rightretvalue = .f.
	_rightretvalue = chkRight(.f.,'MENU',tpad,tbar)
	IF !_rightretvalue
		RETURN
	Endif	
	tbrdesktopmenu.Visible = .F.
	tbrdesktopmenu.timer1.Enabled = .F.
