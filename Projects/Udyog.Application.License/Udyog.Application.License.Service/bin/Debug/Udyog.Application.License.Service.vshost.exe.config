<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <bindings>
      <netTcpBinding>
        <binding name="NetTcpBinding_ILicenseContract">
          <security mode="None">
            <message clientCredentialType="None"/>
            <transport clientCredentialType="None"/>
          </security>
        </binding>        
      </netTcpBinding>
      <!--<wsDualHttpBinding>
        <binding name ="WSHttpBinding_ILicenseService">
          <security mode="None">
            <message clientCredentialType="None"/>
          </security>
        </binding>
      </wsDualHttpBinding>-->
